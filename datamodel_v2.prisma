datasource chinook {
  provider = "sqlite"
  url = "file:../db/flavian.db"
}

model Author {
  id    Int     @id
  name  String?
  posts Post[]
  blog  Blog
}

model Post {
  id    Int      @id
  title String
  tags  String[]
  blog  Blog
}

model Blog {
  id        Int      @id
  name      String
  viewCount Int
  posts     Post[]
  authors   Author[]
}

model PostNews {
  id        Int      @id
}